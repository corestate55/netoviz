{
  "name": "netoviz",
  "version": "0.2.0",
  "description": "RFC8345-based Network Topology Visualizer",
  "author": "corestate55",
  "private": true,
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --exec babel-node",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production babel-node server/index.js",
    "generate": "nuxt generate",
    "format": "prettier --write '{components,layouts,lib,pages,server,store}/**/*.{js,vue}'",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "doc": "jsdoc -c .jsdocrc.json"
  },
  "dependencies": {
    "@babel/node": "^7.7.7",
    "@nuxtjs/axios": "^5.9.2",
    "cross-env": "^6.0.3",
    "d3-drag": "^1.2.5",
    "d3-fetch": "^1.1.2",
    "d3-force": "^2.0.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-timer": "^1.0.10",
    "d3-zoom": "^1.8.3",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "localStorage": "^1.0.4",
    "npm-check-updates": "^4.0.1",
    "nuxt": "^2.11.0",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/vuetify": "^1.9.1",
    "babel-eslint": "^10.0.3",
    "braintree-jsdoc-template": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-plugin-nuxt": ">=0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jsdoc": "^3.6.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prettier-config-standard": "^1.0.1",
    "sequelize-cli": "^5.5.1"
  }
}
