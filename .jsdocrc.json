{
  "opts": {
    "encoding": "utf8",
    "destination": "static/docs",
    "private": false,
    "recurse": true,
    "template": "node_modules/foodoc/template"
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["components", "layouts", "lib", "pages", "server", "store"],
    "exclude": [".nuxt", "bin", "fig", "node_modules", "static"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "((^|\\/|\\\\)_|.*_grpc_(web_)?pb.js$)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
